<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.group07">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Daily Blessings"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Group07">
        <activity android:name=".activites.PreLoginActivity"></activity>
        <activity android:name=".activites.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activites.ViewActivity" />
        <activity android:name=".activites.BrowseActivity" />
        <activity android:name=".activites.MainActivity">
        </activity>
    </application>

</manifest>